    "./v1": "./dist/index.v1.js",
    "./v1beta3": "./dist/index.v1beta3.js",
    "./v1beta4": "./dist/index.v1beta4.js",
    "./v1beta5": "./dist/index.v1beta5.js",
    "./v2beta3": "./dist/index.v2beta3.js"
  }
}
