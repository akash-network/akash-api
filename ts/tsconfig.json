{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2017"
  }
}
